{
  "name": "illusionist/captcha",
  "description": "A captcha library and can be used directly to service Laravel applications.",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "kayson",
      "email": "kayson@huida.cn"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.0",
    "symfony/polyfill-mbstring": "^1.12.0"
  },
  "require-dev": {
    "illuminate/contracts": "^5.0",
    "illuminate/support": "^5.0",
    "mockery/mockery": "^0.9",
    "phpunit/phpunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Illusionist\\Captcha\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Illusionist\\Captcha\\CaptchaServiceProvider"
      ]
    },
    "aliases": {
      "Captcha": "Illusionist\\Captcha\\Facades\\Captcha"
    }
  },
  "config": {
    "sort-packages": true
  }
}
